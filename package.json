{
  "name": "videojs-mobile-ui",
  "version": "0.5.3",
  "description": "Tap controls and fullscreen tweaks for Video.js on mobile",
  "main": "dist/videojs-mobile-ui.cjs.js",
  "module": "dist/videojs-mobile-ui.es.js",
  "repository": "https://github.com/mister-ben/videojs-mobile-ui.git",
  "generator-videojs-plugin": {
    "version": "8.0.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all -s clean -p build:*",
    "build-prod": "cross-env-shell NO_TEST_BUNDLE=1 'npm run build'",
    "build-test": "cross-env-shell TEST_BUNDLE_ONLY=1 'npm run build'",
    "build:css": "postcss -o dist/videojs-mobile-ui.css --config scripts/postcss.config.js src/plugin.css",
    "build:css:bannerize": "bannerize dist/videojs-mobile-ui.css --banner=scripts/banner.ejs",
    "build:js": "rollup -c scripts/rollup.config.js",
    "build:js:bannerize": "bannerize dist/videojs-mobile-ui.js --banner=scripts/banner.ejs",
    "build:js:rollup-modules": "rollup -c scripts/modules.rollup.config.js",
    "build:js:rollup-umd": "rollup -c scripts/umd.rollup.config.js",
    "build:js:uglify": "uglifyjs dist/videojs-mobile-ui.js --comments --mangle --compress  -o dist/videojs-mobile-ui.min.js",
    "build:lang": "vjslang --dir dist/lang",
    "build:test": "rollup -c scripts/test.rollup.config.js",
    "clean": "shx rm -rf ./dist ./test/dist ./cjs ./es && shx mkdir -p ./dist ./test/dist ./cjs ./es",
    "postclean": "mkdirp dist test/dist",
    "docs": "npm-run-all docs:*",
    "docs:api": "jsdoc src -r -d docs/api",
    "docs:toc": "doctoc --notitle README.md",
    "lint": "vjsstandard",
    "server": "karma start scripts/karma.conf.js --singleRun=false --auto-watch",
    "start": "npm-run-all -p server watch",
    "start:server": "node scripts/server.js",
    "pretest": "npm-run-all lint build",
    "test": "npm-run-all lint build-test && karma start scripts/karma.conf.js",
    "posttest": "shx cat test/dist/coverage/text.txt",
    "update-changelog": "conventional-changelog -p videojs -i CHANGELOG.md -s",
    "preversion": "npm test",
    "version": "is-prerelease || npm run update-changelog && git add CHANGELOG.md",
    "watch": "npm-run-all -p watch:*",
    "watch:css": "npm run build:css -- -w",
    "watch:js": "npm run build:js -- -w",
    "watch:js-modules": "rollup -c scripts/modules.rollup.config.js -w",
    "watch:js-umd": "rollup -c scripts/umd.rollup.config.js -w",
    "watch:test": "rollup -c scripts/test.rollup.config.js -w",
    "prepublishOnly": "npm-run-all build-prod && vjsverify --verbose",
    "prepare": "husky install"
  },
  "keywords": [
    "videojs",
    "videojs-plugin"
  ],
  "author": "mister-ben <git@misterben.me>",
  "license": "MIT",
  "vjsstandard": {
    "ignore": [
      "es",
      "cjs",
      "dist",
      "docs",
      "test/dist"
    ]
  },
  "files": [
    "CONTRIBUTING.md",
    "cjs/",
    "dist/",
    "docs/",
    "es/",
    "index.html",
    "scripts/",
    "src/",
    "test/"
  ],
  "dependencies": {
    "global": "^4.4.0",
    "video.js": "^6 || ^7"
  },
  "devDependencies": {
    "@babel/runtime": "^7.14.0",
    "@videojs/generator-helpers": "~2.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "bannerize": "^1.1.3",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-changelog-videojs": "^3.0.0",
    "doctoc": "^1.3.0",
    "husky": "^6.0.0",
    "jsdoc": "^3.6.7",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-detect-browsers": "^2.2.6",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-qunit": "^1.2.1",
    "karma-safari-launcher": "^1.0.0",
    "node-static": "^0.7.10",
    "npm-run-all": "^4.1.2",
    "portscanner": "^2.1.1",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1",
    "qunitjs": "^2.4.1",
    "rollup": "^2.46.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^3.2.2",
    "sinon": "^9.1.0",
    "uglify-js": "^3.3.5",
    "videojs-generate-karma-config": "~8.0.0",
    "videojs-generate-postcss-config": "~3.0.0",
    "videojs-generate-rollup-config": "~6.2.0",
    "videojs-generator-verify": "~3.0.3",
    "videojs-languages": "^2.0.0",
    "videojs-standard": "^8.0.4"
  },
  "browser": "dist/videojs-mobile-ui.js",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.js": "vjsstandard --fix",
    "README.md": "doctoc --notitle"
  }
}
